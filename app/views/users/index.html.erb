<h1>Users list</h1>
<div class="row">
  <div class="col-md-4">
    <table class="table table-striped table-bordered">
      <thead>
        <tr>
          <th>Rating</th>
          <th>User name</th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
          <tr>
            <td>
              <%= user.rating %>
            </td>
            <td>
              <%= link_to user.name, user_path(user) %>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>